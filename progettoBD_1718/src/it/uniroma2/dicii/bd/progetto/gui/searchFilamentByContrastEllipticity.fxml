<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="window" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uniroma2.dicii.bd.progetto.standardUser.SearchFilamentByContrastEllipticityController">
   <children>
      <Label layoutX="126.0" layoutY="62.0" text="Ricerca Filamenti per Contrasto ed Ellitticità:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="24.0" layoutY="20.0" mnemonicParsing="false" onAction="#gotoPreviousMenu" text="Indietro" />
      <Label layoutX="43.0" layoutY="146.0" text="Brillanza minima(%):" />
      <TextField fx:id="tfMinBrightness" layoutX="198.0" layoutY="141.0" onKeyTyped="#clearErrorMessage" />
      <Label layoutX="43.0" layoutY="213.0" text="Ellitticità minima:" />
      <TextField fx:id="tfMinEllipticity" layoutX="198.0" layoutY="208.0" onKeyTyped="#clearErrorMessage" />
      <Label layoutX="43.0" layoutY="279.0" text="Ellitticità massima:" />
      <TextField fx:id="tfMaxEllipticity" layoutX="198.0" layoutY="274.0" onKeyTyped="#clearErrorMessage" />
      <Button layoutX="249.0" layoutY="371.0" mnemonicParsing="false" onAction="#searchFilaments" prefHeight="46.0" prefWidth="86.0" text="Cerca" />
      <Label fx:id="labelErrorMessage" layoutX="26.0" layoutY="539.0" prefHeight="21.0" prefWidth="542.0" textFill="RED" />
      <ProgressIndicator fx:id="progressIndicator" layoutX="385.0" layoutY="368.0" progress="0.0" />
   </children>
</AnchorPane>
