<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="window" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uniroma2.dicii.bd.progetto.standardUser.SearchStarByFilamentController">
   <children>
      <Button layoutX="26.0" layoutY="23.0" mnemonicParsing="false" onAction="#gotoPreviousMenu" prefHeight="30.0" prefWidth="74.0" text="Indietro">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="66.0" layoutY="84.0" text="Specifica il filamento all'interno del quale cercare tutte le stelle:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="filamentName" layoutX="166.0" layoutY="145.0" onKeyTyped="#clearMessage" prefHeight="25.0" prefWidth="378.0" />
      <Label layoutX="80.0" layoutY="148.0" text="Filamento:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="173.0" layoutY="230.0" mnemonicParsing="false" onAction="#searchStarsByFilament" prefHeight="25.0" prefWidth="116.0" text="Ricerca">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="errorMessage" layoutX="166.0" layoutY="186.0" prefHeight="17.0" prefWidth="378.0" textAlignment="CENTER" textFill="RED" />
      <TextArea fx:id="outputArea" editable="false" layoutX="55.0" layoutY="300.0" prefHeight="147.0" prefWidth="491.0" visible="false">
         <font>
            <Font size="14.0" />
         </font></TextArea>
      <ProgressIndicator fx:id="progressIndicator" layoutX="355.0" layoutY="225.0" progress="0.0" />
   </children>
</AnchorPane>
